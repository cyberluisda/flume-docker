<ivysettings>
  <settings defaultResolver='chain-js'/>
  <resolvers>
    <chain name='chain-js' dual="true">
      <ibiblio name="public" m2compatible="true"/>
      <ibiblio name='cloudera' m2compatible='true' root='https://repository.cloudera.com/artifactory/cloudera-repos/' />
        <!-- backup of public for kitesdk libs -->

      <ibiblio name='restlet' m2compatible='true' root='http://maven.restlet.org/' />
        <!-- org.restlet.jee#org.restlet;2.1.1 -->
        <!-- org.restlet.jee#org.restlet.ext.servlet;2.1.1 -->
        
      <ibiblio name='twitter' m2compatible='true' root='http://maven.twttr.com/' />
        <!-- ua_parser#ua-parser;1.3.0 -->
    </chain>
  </resolvers>
</ivysettings>
